@using E_ticaret_sitesi.Models.Siniflar
@model List<KargoTakip>
@{
    ViewBag.Title = "KargoTakip";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Kargo Takip</h2>


<table class="table table-striped">
    <tr>
        <th>Açıklama</th>
        <th>Tarih</th> 
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Aciklama</td>
            <td>@x.TarihZaman</td>
            
        </tr>
    }

</table>